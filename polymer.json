{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/views/agenda-view/agenda-view.html",
    "src/views/asociados-view/asociados-view.html",
    "src/views/servicios-view/servicios-view.html",
    "src/views/error-view.html",
    "src/dialogs/confirm-appointment-dialog.html",
    "src/dialogs/create-appointment-dialog.html",
    "src/dialogs/create-associate-dialog.html",
    "src/dialogs/create-service-dialog.html",
    "src/dialogs/details-appointment-dialog.html",
    "src/dialogs/details-associate-dialog.html",
    "src/dialogs/details-service-dialog.html"
  ],
  "sources": [
    "images/**/*",
    "src/**/*"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "manifest.json"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es6-bundled",
      "js": {
        "compile": false
      }
    }
  ]
}
